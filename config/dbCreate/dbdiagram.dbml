Table Franchise {
  ID integer [pk]
  FranchiseName text
  PrimaryPhone text
  SecondaryPhone text
  FranchiseEmail text
  FranchiseAddress text
}

Table ScrapeSession {
  ID integer [pk]
  StudentID integer [not null] [ref: > Student.ID]
  StartedAt text [not null] [default: '(now())']
  CompletedAt text
  Status text [not null]
}

Table Spreadsheets {
  ID integer [pk]
  FranchiseID integer [ref: > Franchise.ID]
  spreadsheet text
}

Table Student {
  ID integer [pk, increment]
  FranchiseID integer [ref: > Franchise.ID]
  FirstName text [not null]
  LastName text [not null]
  Portal1 text [not null]
  P1Username text [not null, unique]
  P1Password text [not null]
  Portal2 text
  P2Username text
  P2Password text
  YearStart text
  YearEnd text
  WeeklyData text [not null] [default: '{"2025-08-04":{},"2025-08-11":{},"2025-08-18":{},"2025-08-25":{},"2025-09-01":{},"2025-09-08":{},"2025-09-15":{},"2025-09-22":{},"2025-09-29":{},"2025-10-06":{},"2025-10-13":{},"2025-10-20":{},"2025-10-27":{},"2025-11-03":{},"2025-11-10":{},"2025-11-17":{},"2025-11-24":{},"2025-12-01":{},"2025-12-08":{},"2025-12-15":{},"2025-12-22":{},"2025-12-29":{},"2026-01-05":{},"2026-01-12":{},"2026-01-19":{},"2026-01-26":{},"2026-02-02":{},"2026-02-09":{},"2026-02-16":{},"2026-02-23":{},"2026-03-02":{},"2026-03-09":{},"2026-03-16":{},"2026-03-23":{},"2026-03-30":{},"2026-04-06":{},"2026-04-13":{},"2026-04-20":{},"2026-04-27":{},"2026-05-04":{},"2026-05-11":{},"2026-05-18":{},"2026-05-25":{},"2026-06-01":{},"2026-06-08":{},"2026-06-15":{},"2026-06-22":{},"2026-06-29":{}}']
  portal text
}

// Relationships
Ref: ScrapeSession.StudentID > Student.ID
Ref: Spreadsheets.FranchiseID > Franchise.ID
Ref: Student.FranchiseID > Franchise.ID
